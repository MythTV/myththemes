<myththeme>

  <background style="normal">
    <image>ui/background.png</image>
    <buttonarea>256,144,480,360</buttonarea>
    <visiblerowlimit>6</visiblerowlimit>
  </background>

  <titles>
    <position>25,28</position>
    <image mode="TV">title/title_tv.png</image>
    <image mode="MAIN">title/title_main.png</image>
    <image mode="MUSIC">title/title_music.png</image>
    <image mode="GALLERY">title/title_gallery.png</image>
    <image mode="VIDEO">title/title_video.png</image>
    <image mode="GAMES">title/title_games.png</image>
    <image mode="WEATHER">title/title_weather.png</image>
    <image mode="SCHEDULE">title/title_schedule.png</image>
    <image mode="SEARCH">title/title_search.png</image>
    <image mode="WORDS">title/title_search.png</image>
    <image mode="LISTS">title/title_search.png</image>
    <image mode="SETUP">title/title_setup.png</image>
    <image mode="TVSETUP">title/title_tv.png</image>
    <image mode="SETUP_TVRECPRIORITIES">title/title_priority.png</image>
    <image mode="MEDIA_SETUP">title/title_multimedia.png</image>
    <image mode="INFO_SETUP">title/title_info_settings.png</image>
    <image mode="DVD">title/title_dvd.png</image>
    <image mode="DVD_SETTINGS">title/title_dvd.png</image>
    <image mode="MUSIC_SETTINGS">title/title_music_setup.png</image>
    <image mode="VIDEO_SETTINGS">title/title_video.png</image>
    <image mode="GAME_SETTINGS">title/title_games.png</image>
    <image mode="MANAGE_RECORDINGS">title/title_schedule.png</image>
    <image mode="LIBRARY">title/title_multimedia.png</image>
    <image mode="INFO">title/title_info_center.png</image>
    <image mode="OPTICAL_DISK">title/title_dvd.png</image>
    <image mode="UTIL">title/title_setup.png</image>
    <image mode="NETFLIX">title/title_dvd.png</image>
  </titles>

  <genericbutton> <!-- Size: 480 x 60 px -->
    <watermarkposition>832,196</watermarkposition>
    <normal>ui/button_off.png</normal>
    <active>ui/button_on.png</active>
    <text>
      <area>16,16,0,0</area>
      <color>white</color>
      <fontname>Trebuchet MS</fontname>
      <fontsize>20</fontsize>
      <bold>yes</bold>
      <italics>no</italics>
      <!--
	  <outline>
         <color>black</color>
         <size>1</size>
      </outline>-->
      <shadow>
         <color>black</color>
         <offset>2,2</offset>
         <alpha>50</alpha>
      </shadow>
    </text>
  </genericbutton>
  
  <logo> <!-- Size: 440 x 92 px -->
    <image>ui/mythtv_logo.png</image>
    <position>48,594</position>
  </logo>
  
  <uparrow> <!-- Size: 50 x 50 px -->
    <image>ui/up.png</image>
    <position>686,99</position>
  </uparrow>

  <downarrow> <!-- Size: 50 x 50 px -->
    <image>ui/down.png</image>
    <position>686,499</position>
  </downarrow>
  
